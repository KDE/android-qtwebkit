        KWQKComboBox.o \
        KWQKCompletionBox.o \
        KWQKConfigBase.o \
        KWQKCursor.o \
        KWQKDebug.o \
        KWQKGlobal.o \
	KWQKGlobalSettings.o \
        KWQKHTMLPart.o \
        KWQKIconLoader.o \
        KWQKInstance.o \
        KWQKLineEdit.o \
        KWQKListBox.o \
